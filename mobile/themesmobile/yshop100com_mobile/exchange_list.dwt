<!---返回顶部-->
<script>
function goTop(){
  $('html,body').animate({'scrollTop':0},600);
}

//显示分类
function show_keywords() {
  var bd_top = $(document).scrollTop();
  if($('.keywords_nav').css('display')=='none') {
    $('.keywords_nav').removeClass('hid');
    $('.keywords_nav').addClass('show');
  } else {
    $('.keywords_nav').removeClass('show');
    $('.keywords_nav').addClass('hid');
  }
 }

(function(){
   var $nav = $('.keywords_nav');
   $(window).on("scroll", function() {  
   $('.keywords_nav').removeClass('show');  
  $('.keywords_nav').addClass('hid');
  });
 })();

</script>
<a href="javascript:goTop();" class="gotop"><img src="themesmobile/yshop100com_mobile/images/topup.png"></a> 
